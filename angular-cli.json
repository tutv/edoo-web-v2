{
    "project": {
        "version": "1.0.0-beta.17",
        "name": "angular-web-v2"
    },
    "apps": [
        {
            "root": "src",
            "outDir": "dist",
            "assets": "assets",
            "index": "index.html",
            "main": "main.ts",
            "test": "test.ts",
            "tsconfig": "tsconfig.json",
            "prefix": "app",
            "mobile": false,
            "styles": [
                "assets/css/font-awesome.css",
                "assets/css/animate.css",
                "assets/css/nprogress.css",
                "../node_modules/ng2-datetime/src/vendor/bootstrap-datepicker/bootstrap-datepicker3.min.css",
                "../node_modules/ng2-datetime/src/vendor/bootstrap-timepicker/bootstrap-timepicker.min.css",
                "styles.css"
            ],
            "scripts": [
                "../node_modules/jquery/dist/jquery.js",
                "assets/js/nprogress.js",
                "assets/js/jquery.form.js",
                "assets/js/jquery.noty.packaged.min.js",
                "assets/js/theme.noty.js",
                "../node_modules/bootstrap/dist/js/bootstrap.js",
                "assets/js/main.js",
                "../node_modules/tinymce/tinymce.jquery.js",
                "../node_modules/tinymce/themes/modern/theme.js",
                "../node_modules/tinymce/plugins/link/plugin.js",
                "../node_modules/tinymce/plugins/image/plugin.js",
                "../node_modules/tinymce/plugins/paste/plugin.js",
                "../node_modules/tinymce/plugins/table/plugin.js",
                "../node_modules/ng2-datetime/src/vendor/bootstrap-datepicker/bootstrap-datepicker.min.js",
                "../node_modules/ng2-datetime/src/vendor/bootstrap-timepicker/bootstrap-timepicker.min.js",
                "assets/js/google-charts-loader.js"
            ],
            "environments": {
                "source": "environments/environment.ts",
                "dev": "environments/environment.ts",
                "prod": "environments/environment.prod.ts"
            }
        }
    ],
    "addons": [],
    "packages": [],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "css",
        "prefixInterfaces": false
    }
}
